<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.tpfinal.modelo.Articulo.java" table="Articulo">
        <id name="IDarticulo" column="IDarticulo">
        
        </id>
        <property name="Nombre" type="string" column="nombre" />
        <property name="Marca" type="string" column="marca" />
        <property name="Precio" type="float" column="precio"/>
        <property name="Stock" type="int" column="stock"/>
        <property name="Observaciones" type="string" column="observaciones"/>
        <property name="Descripcion" type="string" column="descripcion"/>
        <property name="Actividad" type="string" column="actividad"/>
        <property name="DiaAlta" type="String" column="diaAlta"/>
        <property name="MesAlta" type="string" column="mesAlta"/>
        <property name="AñoAlta" type="string" column="añoAlta"/>
    </class>
</hibernate-mapping>
