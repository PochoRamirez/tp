<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.tpfinal.modelo.Empleado" table="empleado">
        <id name="idEmpleado" column="idEmpleado" type="integer">
            <generator class="increment" />
        </id>
        <property name="Administrador" type="boolean" column="administrador" />
        <property name="Contraseña" type="string" column="contraseña" />
        <property name="Dni" type="integer" column="dni" />
        <property name="Nombre" type="string" column="nombre" />
        <property name="Apellido" type="string" column="apellido" />
        <property name="Domicilio" type="string" column="domicilio"/>
        <property name="Mail" type="string" column="mail"/>
        <property name="fechaNacimiento" type="date" column="fechaNacimiento"/>
        <property name="Actividad" type="boolean" column="actividad"/>
        <property name="Telefono" type="integer" column="telefono"/>
    </class>
</hibernate-mapping>
