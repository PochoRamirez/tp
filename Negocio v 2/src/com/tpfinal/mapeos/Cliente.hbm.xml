<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.tpfinal.modelo.Cliente" table="cliente">
        <id name="IdCliente" column="IdCliente">
        
        </id>
        <property name="DNI" type="string" column="dni" />
        <property name="Nombre" type="string" column="nombre" />
        <property name="Apellido" type="string" column="apellido" />
        <property name="Telefono" type="string" column="telefono"/>
        <property name="Domicilio" type="string" column="domicilio"/>
        <property name="DiaNacimiento" type="string" column="diaNacimiento"/>
        <property name="MesNacimiento" type="string" column="mesNacimiento"/>
        <property name="AñoNacimiento" type="string" column="añoNacimiento"/>
        <property name="Mail" type="string" column="mail"/>
        <property name="fechaAlta" type="date" column="fechaAlta"/>
       
    </class>
</hibernate-mapping>